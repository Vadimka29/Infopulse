
public class SPoint {
	private int x,y;
	
	public SPoint(int x, int y){
		this.x = x;
		this.y = y;
	}
	
	@Override
	public String toString(){
		return "[x = " + x + " y = " + y + "]";
	}
	
	@Override
	public int hashCode(){
		return x + y;
	}
	public boolean equals(Object ob){
		if(ob == this)
			return true;
		if()
	}
}
