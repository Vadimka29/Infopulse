import java.io.File;

public class Methods {

	public static void Directory(){
		StringBuilder path =  new StringBuilder();
		path.append("/media/vadim/12EAE1C3EAE1A369");
		for(int i = 0; i < 4; i ++){
			path.append("/" + i);
		}
		File f = new File(path.toString());
		System.out.println(f.getPath());
		}
	}
